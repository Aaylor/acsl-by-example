predicate P_EqualRanges_2_
    (Mint_0:(addr,int) farray,
    Mint_1:(addr,int) farray,
    a:addr,
    n:int) =
    forall i : int. let a_1 = shift_sint32(a, i) : addr in (i < n) ->
    (0 <= i) -> (Mint_0[a_1] = Mint_1[a_1])

